<h2>Edit</h2>

<div class="row">
    <%= form_for @user do |f| %>
    <div class="row">
      <div class="col-sm">
        <%= f.label :first_name %>
      </div>
      <div class="col-sm">
        <%= f.text_field :first_name, class: "form-control"%>
      </div>
    </div>

    <div class="row">
      <div class="col-sm">
        <%= f.label :last_name%>
      </div>
      <div class="col-sm">
        <%= f.text_field :last_name, class: "form-control"%>
      </div>
    </div>

    <div class="row">
      <div class="col-sm">
        <%= f.label :date_of_birth%>
      </div>
      <div class="col-sm">
        <%= f.date_field :date_of_birth, class: "form-control"%>
      </div>
    </div>

    <div class="row">
      <div class="col-sm">
        <%= f.label :email%>
      </div>
      <div class="col-sm">
        <%= f.email_field :email, class: "form-control"%>
      </div>
    </div>

    <div class="row">
      <div class="col-sm">
        <%= f.label :phone_number%>
      </div>
      <div class="col-sm">
        <%= f.text_field :phone_number, class: "form-control"%>
      </div>
    </div>
    <%= f.submit class: "btn btn-primary"%>

    <%end%>
</div>
